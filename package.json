{
    "name": "tablenow-backend",
    "version": "1.0.0",
    "description": "TableNow AI Restaurant SaaS Backend",
    "main": "dist/server.js",
    "scripts": {
        "dev": "nodemon src/server.ts",
        "clean": "rimraf dist",
        "copy-files": "copyfiles -u 1 src/**/*.json src/**/*.ejs src/**/*.pem dist/",
        "build": "npm run clean && tsc && npm run copy-files",
        "start": "node dist/server.js",
        "setup-db": "ts-node src/scripts/setupDatabase.ts",
        "heroku-prebuild": "npm install",
        "heroku-postbuild": "npm run build",
        "render-build": "npm install && npm run build"
    },
    "keywords": [
        "restaurant",
        "ai",
        "saas",
        "vapi",
        "booking"
    ],
    "author": "",
    "license": "MIT",
    "dependencies": {
        "@google/generative-ai": "^0.1.3",
        "@hubspot/api-client": "^11.0.0",
        "@pinecone-database/pinecone": "^2.0.1",
        "@sendgrid/mail": "^8.1.0",
        "@supabase/supabase-js": "^2.87.1",
        "axios": "^1.6.2",
        "bcryptjs": "^2.4.3",
        "cors": "^2.8.5",
        "dotenv": "^16.3.1",
        "express": "^4.18.2",
        "express-rate-limit": "^7.1.5",
        "googleapis": "^128.0.0",
        "helmet": "^7.1.0",
        "ical-generator": "^6.0.1",
        "jsonwebtoken": "^9.0.2",
        "langchain": "^0.1.0",
        "mailparser": "^3.6.5",
        "mammoth": "^1.6.0",
        "morgan": "^1.10.0",
        "multer": "^1.4.5-lts.1",
        "nodemailer": "^6.9.7",
        "openai": "^4.24.1",
        "pdf-parse": "^1.1.1",
        "uuid": "^9.0.1"
    },
    "devDependencies": {
        "@types/bcryptjs": "^2.4.6",
        "@types/cors": "^2.8.17",
        "@types/express": "^4.17.21",
        "@types/jsonwebtoken": "^9.0.5",
        "@types/node": "^20.10.6",
        "@types/nodemailer": "^6.4.14",
        "@types/nodemailer-sendgrid": "^1.4.4",
        "@types/uuid": "^9.0.7",
        "copyfiles": "^2.4.1",
        "nodemon": "^3.0.2",
        "rimraf": "^5.0.5",
        "ts-node": "^10.9.2",
        "typescript": "^5.3.3"
    }
}